/* 숨기기 */

.custom-acotianOn{
	overflow:hidden;
	max-height:800px;
	transition:all 0.35s;
}

.custom-acotianOff{
	overflow:hidden;
	max-height :0px;
	transition : all 0.35s;
}

.content-fade{
	display:none;
}
.content-unfade{
	display:black;
}

