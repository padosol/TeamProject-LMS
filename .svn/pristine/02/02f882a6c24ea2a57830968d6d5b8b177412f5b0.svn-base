@charset "UTF-8";
.table-timeTable{
	width:100%;
	height:100%;
	table-layout:fixed;
	font-size:12px;
}

.cal-title {
	font-weight:bold;
	font-size:18px;
	color:black;
}

.table-timeTable tr{
	height:40px;	
}


.table-timeTable tr td:nth-child(n+2){
	border: 2px solid #cdcaca;
}

.table-timeTable tr td:nth-child(1){
	text-align:right;
	padding-right:20px;
}

.clickBg{
	background-color:#c7dfed;	
}

.partColor{
	background-image: linear-gradient(to right, #e9fff1 80%, white);
	z-index:2;
}

.bg-darkGreen {
	background-color:#004130;
}

.custom-overflow-auto{
	overflow:auto;
}
.custom-table-sm{
	width:100%;
	table-layout:fixed;
	min-width:50px;
}
.custom-table-sm td{
	width: 100px;
	min-width : 50px;
	height:25px;
	min-hieght:50px;
	overflow:hidden;
	text-overflow: ellipsis;
}

.custom-fontColor-white{
	color: #fff;
}

.custom-table-height-35 td{
	height:35px;
}

.custom-table{
	width:100%;
	height:700px;
	table-layout: fixed;
	min-width:300px;
}

.custom-table td {
	width: 100px;
	min-width : 50px;
	height:50px;
	min-hieght:50px;
	overflow:hidden;
	text-overflow: ellipsis;
}

/* 클릭시 파장 */
.click-wave{
	position:absolute;
	width:30px;height:30px;
	transform:translate(-50%, -50%);
	border-radius:50%;
	border:2px solid red;
	z-index:100;
	animation : circleSize 0.25s ease;
}


.mini-timeTable{
	width:100%;
	min-width:50px;
	height : 500px;
	table-layout:fixed;
}

.mini-timeTable td{
	overflow:hidden;
	white-space : nowrap;
	text-overflow: ellipsis;
}


@keyframes circleSize {
	0%{
		width:0;
		height:0;
	}
	100%{
	    width:30px;
	    height:30px;
	}
}


/* div 테두리 둥글게 */
.custom-div-radius{
	border-radius : 20px;
}

